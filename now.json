{
  "version": 2,
  "public": false,
  "name": "animations",
  "alias": ["animations"],
  "regions": ["bru1"],
  "builds": [{ "src": "package.json", "use": "@now/static-build" }],
  "routes": [
    { "src": "^/css/(.*)", "dest": "/css/$1" },
    { "src": "^/js/(.*)", "dest": "/js/$1" },
    { "src": "/robots.txt", "dest": "/robots.txt" },
    { "src": "/manifest.json", "dest": "/manifest.json" },
    { "src": "/favicon.ico", "dest": "/favicon.ico" },
    { "src": ".*", "dest": "/index.html" }
  ]
}
